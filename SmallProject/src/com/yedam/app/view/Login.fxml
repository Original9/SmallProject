            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" layoutX="250.0" layoutY="287.0" prefHeight="19.0" prefWidth="300.0" text="Status" textFill="#d31c1c">
